    <!-- HelpActivity -->
    <string name="help_overview_heading">Tổng quát</string>
    <string name="help_intro">Ứng dụng này có thể đếm số bước của bạn trong nền, cung cấp cho bạn cái nhìn tổng quan về số bước đã đi bộ của bạn và cho phép bạn xác định các chế độ đi bộ tùy chỉnh và thông báo nếu việc đạt được mục tiêu số bước hàng ngày của bạn đang gặp nguy hiểm.</string>
    <string name="help_functionality_step_counter_summary">Bộ đếm bước đếm số bước hàng ngày của bạn trong nền và có thể bị tắt trong cài đặt.</string>
    <string name="help_functionality_notification_heading">Thông báo vĩnh viễn</string>
    <string name="help_functionality_notification">Thông báo</string>
    <string name="help_functionality_notification_summary">Thông báo vĩnh viễn là cần thiết để giữ cho bộ đếm bước hoạt động trong nền. Nội dung của thông báo có thể được chọn trong cài đặt.</string>
    <string name="help_functionality_notification_delete">Bạn có thể tắt tất cả thông báo trong tùy chọn hệ thống.</string>
    <string name="help_functionality_motivation_alert">Cảnh báo động lực</string>
    <string name="help_functionality_motivation_alert_summary">Ứng dụng có thể hiển thị cho bạn cảnh báo về động lực nếu việc đạt được mục tiêu số bước hàng ngày của bạn đang gặp nguy hiểm. Bạn có thể xác định tiêu chí, thời gian trong ngày và văn bản thông báo.</string>
    <string name="help_privacy_heading">Thông tin về quyền riêng tư</string>
    <string name="help_permission_boot_heading">Quyền: Nhận khởi động đã hoàn thành</string>
    <string name="help_permission_boot_description">được sử dụng để khởi động bộ đếm bước và đặt thông báo tại thời điểm khởi động. Nếu quyền không được cấp, bạn phải khởi động ứng dụng theo cách thủ công một lần sau khi khởi động để kích hoạt ứng dụng.</string>
    <string name="help_permissions_selection">Người dùng không thể quản lý các quyền được sử dụng bởi Máy đếm bước chân thân thiện với quyền riêng tư trong bất kỳ phiên bản Android nào.</string>
    <string name="help_permission_wake_heading">Quyền: Khóa đánh thức</string>
    <string name="help_permission_wake_description">được sử dụng để giữ cho CPU tỉnh táo để đếm các bước trong nền.</string>
    <string name="help_permission_gps_heading">Quyền: Vị trí chính xác</string>
    <string name="help_permission_gps_description">được sử dụng để hiển thị tốc độ đi bộ hiện tại của bạn trên màn hình chính. Nếu không được cấp phép, tốc độ đi bộ sẽ không được hiển thị - không có tác động nào khác.</string>

